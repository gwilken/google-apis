            },
            "borderRight": {
              "color": {},
              "width": { "unit": "PT" },
              "padding": { "unit": "PT" },
              "dashStyle": "SOLID"
            },
            "keepLinesTogether": false,
            "keepWithNext": false,
            "shading": {
              "backgroundColor": {
                "color": {
                  "rgbColor": {
