          "elements": [
            {
              "startIndex": 323,
              "endIndex": 340,
              "textRun": {
                "content": "General Practice\n",
                "textStyle": { "fontSize": { "magnitude": 17, "unit": "PT" } }
              }
            }
          ],
          "paragraphStyle": {
            "headingId": "h.ntaagppqxych",
            "namedStyleType": "HEADING_2",
