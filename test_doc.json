      {
        "startIndex": 1932,
        "endIndex": 2203,
        "paragraph": {
          "elements": [
            {
              "startIndex": 1932,
              "endIndex": 1958,
              "textRun": {
                "content": "Employees who qualify for ",
                "textStyle": { "fontSize": { "magnitude": 12, "unit": "PT" } }
              }
            },
            {
              "startIndex": 1958,
              "endIndex": 1971,
              "textRun": {
                "content": "job-protected",
                "textStyle": {
                  "italic": true,
                  "fontSize": { "magnitude": 12, "unit": "PT" }
                }
              }
            },
            {
              "startIndex": 1971,
              "endIndex": 2203,
              "textRun": {
                "content": " leave under the Family Medical Leave Act can be required to use their earned and unused vacation, sick or personal time to cover the leave period. The employer's policy governs the amount of PTO that can be used for FMLA purposes.\n",
                "textStyle": { "fontSize": { "magnitude": 12, "unit": "PT" } }
              }
            }
          ],
          "paragraphStyle": {
