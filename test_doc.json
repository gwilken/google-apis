          "elements": [
            {
              "startIndex": 1126,
              "endIndex": 1153,
              "textRun": {
                "content": "Use-It-Or-Lose It Policies\n",
                "textStyle": { "fontSize": { "magnitude": 17, "unit": "PT" } }
              }
            }
          ],
          "paragraphStyle": {
            "headingId": "h.maxdubf92w1s",
