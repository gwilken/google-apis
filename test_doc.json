            {
              "startIndex": 2491,
              "endIndex": 2522,
              "textRun": {
                "content": "Holidays and Inclement Weather\n",
                "textStyle": { "fontSize": { "magnitude": 17, "unit": "PT" } }
              }
            }
          ],
          "paragraphStyle": {
            "headingId": "h.q8va3z4eonm1",
